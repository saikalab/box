      {
        "rule_set": "cn_domain",
        "server": "dns-ali"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns-fakeip"
      }
    ],
    "final": "dns-cloudflare",
    "reverse_mapping": true
  },
